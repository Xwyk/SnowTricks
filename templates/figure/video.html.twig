{% block _figure_videos_entry_row %}
    <fieldset class="form-group">
        <div class="row">
            <div class="col-10">
                {{ form_label(form.url) }}
            </div>
            <div class="col-2 removeBtn">
            </div>
        </div>
        <div class="row">
            <div class="col-2">
                <div class="videoOnEdit">
                    {{ form.url.vars.data|raw }}
                </div>
                {{ form_errors(form.url) }}
                {{ form_widget(form.url) }}
            </div>
        </div>
    </fieldset>
{% endblock %}